<table>
    <thead>
        <th>Name</th>
        <th>Job Title</th>
        <th>Twitter</th>
    </thead>
    <tbody>
        {{#users}}
        <tr>
            <td>{{person.firstName}}</td>
            <td>{{jobTitle}}</td>
            <td><a href="https://twitter.com/{{twitter}}">@{{twitter}}</a></td>
        </tr>
        {{/users}}
    </tbody>
</table>
